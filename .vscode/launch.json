    "version": "0.2.0",
    "configurations": [
        {
            "name": "data.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/examples/ScanNet/data.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Remote Attach",
            "type": "python",
            "request": "attach",
            "connect": { "host": "localhost", "port": 5678 },
            "pathMappings": [{ "localRoot": "${workspaceFolder}", "remoteRoot": "/app/src" }]
        },
    ]
}